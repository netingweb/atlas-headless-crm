{
  "tenant_id": "demo2",
  "name": "Demo2 Automotive Tenant",
  "settings": {},
  "embeddingsProvider": {
    "name": "openai",
    "apiKey": "",
    "model": "text-embedding-3-small"
  },
  "storage": {
    "type": "minio",
    "config": {
      "endpoint": "localhost:9000",
      "accessKey": "minioadmin",
      "secretKey": "minioadmin",
      "useSSL": false,
      "bucket": "documents"
    }
  },
  "visionProvider": {
    "name": "openai",
    "model": "gpt-4-vision-preview",
    "apiKey": ""
  },
  "documentProcessing": {
    "maxFileSize": 104857600,
    "allowedMimeTypes": [
      "application/pdf",
      "image/*",
      "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
      "text/plain",
      "text/csv"
    ],
    "chunkingDefaults": {
      "chunkSize": 1000,
      "chunkOverlap": 200
    }
  }
}
